{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },

    { "src": "^/api/(.*)$", "dest": "/api/$1" },

    { "src": "^/js/(.*)$", "dest": "/frontend/js/$1" },
    { "src": "^/public/(.*)$", "dest": "/frontend/public/$1" },

    { "src": "^/$", "dest": "/frontend/pages/index.html" },
    { "src": "^/auth$", "dest": "/frontend/pages/auth.html" },
    { "src": "^/login$", "dest": "/frontend/pages/login.html" },
    { "src": "^/signup$", "dest": "/frontend/pages/signup.html" },
    { "src": "^/admin$", "dest": "/frontend/pages/admin.html" },
    { "src": "^/app$", "dest": "/frontend/pages/app.html" }
  ]
}
